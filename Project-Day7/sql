SHOW DATABASES;
CREATE DATABASE pythontest;
DROP DATABASE pythontest;

SELECT DATABASE() FROM dual;

CREATE TABLE Student(
Rollno int PRIMARY KEY,
SName varchar(20),
Mark int
);

DESC Student;

